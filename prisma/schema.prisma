datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Player {
  id           Int      @id @default(autoincrement())
  apiId        Int      @unique
  name         String
  team         String
  position     String
  goals        Int?
  assists      Int?
  totalPoints  Float?
}